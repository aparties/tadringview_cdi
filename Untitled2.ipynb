    "def _zero_gex(strikes):\n",
    "    def _aux_add(a, b):\n",
    "        return (b[0], a[1] + b[1])\n",
    "\n",
    "    cumsum = list(itertools.accumulate(strikes, _aux_add))\n",
    "    if cumsum[len(strikes) // 10][1] < 0:\n",
    "        op = min\n",
    "    else:\n",
    "        op = max\n",
    "    return op(cumsum, key=lambda i: i[1])[0]"
   ]
